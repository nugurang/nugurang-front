{
  "name": "nugurang",
  "version": "0.5.0",
  "main": "index.js",
  "repository": "https://github.com/nugurang/nugurang-front.git",
  "license": "MIT",
  "scripts": {
    "__generate_resolvers": "graphql-codegen --config .codegen/resolvers.yml",
    "__mock_server": "env-cmd -e common,mock --use-shell \"npx ts-node -r tsconfig-paths/register src/mocks/standalone.ts\"",
    "__mock_client": "env-cmd -e common,mock --use-shell \"$npm_execpath run __browser\"",
    "__browser": "next dev",
    "__build": "next lint && next build && next export && npx cap sync",
    "init": "$npm_execpath run __generate_resolvers",
    "mock": "concurrently \"$npm_execpath run __mock_server\" \"$npm_execpath run __mock_client\"",
    "local": "env-cmd -e common,local --use-shell \"$npm_execpath run __browser\"",
    "dev": "env-cmd -e common,dev --use-shell \"$npm_execpath run __browser\"",
    "build:mock": "env-cmd -e common,mock --use-shell \"$npm_execpath run __build\"",
    "build:local": "env-cmd -e common,local --use-shell \"$npm_execpath run __build\"",
    "build:dev": "env-cmd -e common,dev --use-shell \"$npm_execpath run __build\""
  },
  "dependencies": {
    "@apollo/client": "^3.7.2",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/material": "^5.11.6",
    "concurrently": "^7.6.0",
    "deepmerge": "^4.2.2",
    "env-cmd": "^10.1.0",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "graphql": "^16.6.0",
    "immer": "^9.0.16",
    "next": "13.0.6",
    "next-i18next": "^13.0.0",
    "nookies": "^2.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@fortawesome/fontawesome-common-types": "^6.2.1",
    "@graphql-codegen/cli": "^2.16.1",
    "@graphql-codegen/typescript": "^2.8.5",
    "@graphql-codegen/typescript-msw": "^1.1.6",
    "@graphql-codegen/typescript-operations": "^2.5.10",
    "@graphql-codegen/typescript-resolvers": "^2.7.10",
    "@mswjs/http-middleware": "^0.6.0",
    "@types/node": "18.11.13",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "husky": "^8.0.2",
    "msw": "^0.49.1",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "4.9.4"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
